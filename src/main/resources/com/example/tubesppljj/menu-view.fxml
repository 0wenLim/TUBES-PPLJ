<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tubesppljj.MenuView">

    <Label id="welcome" fx:id="welcomeText" text="MENU">
        <font>
            <Font size="20.0" />
        </font></Label>
    <Button fx:id="newButton" onMouseClicked="#onNewButtonClick" text="Create New Room" />
    <Button fx:id="joinButton" onMouseClicked="#onJoinButtonClick" text="Join Room" />
    <Button fx:id="histButton" onMouseClicked="#onHistoryButtonClick" text="History" />

</VBox>
